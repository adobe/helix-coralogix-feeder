{
  "name": "@adobe/helix-coralogix-feeder",
  "version": "2.1.0",
  "private": true,
  "description": "Helix Coralogix Feeder",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon",
    "test": "c8 mocha  -i -g 'Post-Deploy' --spec=test/**/*.test.js",
    "test-postdeploy": "mocha -g 'Post-Deploy' --spec=test/**/*.test.js",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "build": "hedy -v --test-bundle",
    "deploy": "hedy -v --deploy --test --cleanup-patch=1w",
    "deploy-routes": "hedy --no-build --no-hints -l major",
    "deploy-ci": "hedy -v --deploy --test --pkgVersion=ci$CIRCLE_BUILD_NUM -l ci --cleanup-ci 48h",
    "prepare": "husky"
  },
  "wsk": {
    "target": "aws",
    "name": "helix3/coralogix-feeder@${version}",
    "memory": 1024,
    "testUrl": "/_status_check/healthcheck.json",
    "awsAttachAuthorizer": "helix-token-authorizer_v2",
    "fastlyServiceId!important": "",
    "nodeVersion": 22
  },
  "mocha": {
    "require": [
      "test/setup-env.js",
      "mocha-suppress-logs"
    ],
    "recursive": "true",
    "reporter": "mocha-multi-reporters",
    "reporter-options": "configFile=.mocha-multi.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-coralogix-feeder"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-coralogix-feeder/issues"
  },
  "homepage": "https://github.com/adobe/helix-coralogix-feeder#readme",
  "dependencies": {
    "@adobe/fetch": "4.2.3",
    "@adobe/helix-shared-wrap": "2.0.2",
    "@adobe/helix-status": "10.1.5",
    "aws4": "1.13.2",
    "fetch-retry": "6.0.0"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.12",
    "@adobe/helix-deploy": "13.1.17",
    "@adobe/helix-universal": "5.2.3",
    "@adobe/helix-universal-devserver": "1.1.141",
    "@adobe/semantic-release-coralogix": "1.1.40",
    "@eslint/config-helpers": "0.4.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "10.0.1",
    "c8": "10.1.3",
    "dotenv": "17.2.2",
    "eslint": "9.4.0",
    "husky": "9.1.7",
    "junit-report-builder": "5.1.1",
    "lint-staged": "16.2.3",
    "mocha": "11.7.2",
    "mocha-multi-reporters": "1.5.1",
    "mocha-suppress-logs": "0.6.0",
    "nock": "13.5.6",
    "nodemon": "3.1.10",
    "semantic-release": "24.2.9"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "nodemonConfig": {
    "exec": "node --inspect ./test/dev/server.mjs",
    "watch": [
      ".env",
      "."
    ],
    "ext": ".js, .cjs, .ejs, .css"
  }
}
